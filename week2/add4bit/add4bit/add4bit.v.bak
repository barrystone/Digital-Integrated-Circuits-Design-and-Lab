module add4bit(cout,sum,a,b);

	output [3:0] sum;
	output cout;
	input [3:0] a;
	input [3:0] b;
	
	assign sum[0] = a[0] ^ b[0] ;
	assign cout =(a[0]&b[0]);
	
	assign sum[1] = a[1] ^ b[1];
	assign cout =(a[1]&b[1]);
	
	assign sum[2] = a[2] ^ b[2];
	assign cout =(a[2]&b[2]);
	
	assign sum[3] = a[3] ^ b[3];
	assign cout =(a[3]&b[3]);
	
endmodule
	